{
  "expo": {
    "name": "Scripsail",
    "slug": "scripsail",
    "scheme": "scripsail",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "backgroundColor": "#fff",
      "image": "./assets/splash.png"
    },
    "icon": "./assets/icon.png",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Scripsail wants to access your photos."
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Scripsail to access your camera."
        }
      ],
      "expo-secure-store",
      "expo-localization"
    ],
    "ios": {
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app uses your location to show it on the map.",
        "NSCameraUsageDescription": "This app uses the camera to scan student ID cards."
      },
      "config": {
        "googleMapsApiKey": "AIzaSyCu96pvVcFvKQy4awjhODZmtK1_KR-xfB0"
      },
      "bundleIdentifier": "com.junweialreadyexist.scripsail"
    },
    "android": {
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.CAMERA"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCu96pvVcFvKQy4awjhODZmtK1_KR-xfB0"
        }
      },
      "jsEngine": "hermes",
      "package": "com.junweialreadyexist.scripsail"
    },
    "extra": {
      "eas": {

      }
    }
  }
}
